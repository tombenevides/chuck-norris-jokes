{
    "name": "tombenevides/chucknorrisjokes",
    "description": "A package that creates random Chuck Norris jokes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Benevides",
            "email": "eltonfelixbenevides@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Tombenevides\\ChuckNorrisJokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tombenevides\\ChuckNorrisJokes\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tombenevides\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "ChuckNorrisJokes\\ChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }
}
